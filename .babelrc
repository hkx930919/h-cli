{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          //   "edge": "17",
          //   "firefox": "60",
          //   "chrome": "67",
          //   "safari": "11.1"
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
        // "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "vant",
        "libraryDirectory": "es",
        "style": true
      }
    ],
    [
      "component",
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ]
  ]
}
